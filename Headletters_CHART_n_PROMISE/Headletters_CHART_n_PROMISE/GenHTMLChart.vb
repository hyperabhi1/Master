        Dim HIDUIDDateTime = UID + "_" + HID
        Dim fPath0 = "C:\Astro\Templates\" + HIDUIDDateTime + "_Chart.html"
        Dim fPath1 = "C:\Astro\Templates\" + HIDUIDDateTime + "_Dasha.html"
        Dim fPath2 = "C:\Astro\Templates\" + HIDUIDDateTime + ".html"
        Dim fPath3 = "C:\Astro\Templates\" + HIDUIDDateTime + ".pdf"
        If IO.File.Exists(fPath0) = True Then
            IO.File.Delete(fPath0)
        End If
        If IO.File.Exists(fPath1) = True Then
            IO.File.Delete(fPath1)
        End If
        If IO.File.Exists(fPath2) = True Then
            IO.File.Delete(fPath2)
        End If
        If IO.File.Exists(fPath3) = True Then
            IO.File.Delete(fPath3)
        End If
        Dim afile0 As New IO.StreamWriter(fPath0, True)
        Dim afile1 As New IO.StreamWriter(fPath1, True)
        Dim afile2 As New IO.StreamWriter(fPath2, True)
        Dim Charttext = HTMLStringBuilder(HIDUIDDateTime, HID, UID, P_List, H_List, BirthLagna, BirthBhav, BirthSouth, DasaListP, personalDetails)
        Dim PDFVwtext = PrintableHTMLStringBuilder(HIDUIDDateTime, HID, UID, P_List, H_List, BirthLagna, BirthBhav, BirthSouth, DasaListP, personalDetails)
        Dim Dashatext = DashaStringBuilder(DasaListP, personalDetails)
        afile0.WriteLine(Charttext)
        afile1.WriteLine(Dashatext)
        afile2.WriteLine(PDFVwtext)
        afile0.Close()
        afile1.Close()
        afile2.Close()
        RunCommandCom(HIDUIDDateTime)
        DBController.UpdateChartLink(HIDUIDDateTime, HID, UID)
    End Sub

